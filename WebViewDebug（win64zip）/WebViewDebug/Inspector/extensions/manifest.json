{
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "description": "webview inspector",
    "devtools_page": "main.html",
    "manifest_version": 2,
    "name": "webview debug tools",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "chrome-extension://*",
        "file:///*",
        "http://*/*",
        "https://*/*",
        "chrome://*"
    ],
    "background": {
        "scripts": [
            "./js/background.js"
        ]
    },
    "version": "0.0.1",
    "web_accessible_resources": [
        "main.html",
        "panel.html",
        "180870/*"
    ]
}
